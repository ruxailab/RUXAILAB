    "csvtojson": "^2.0.10",
    "email-templates": "^7.2.0",
    "firebase": "^9.23.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.7.0",
    "handlebars": "^4.7.8",
    "jsdoc-vuex-plugin": "^1.0.0",
    "material-components-web": "^14.0.0",
    "nodemailer": "^6.9.5",
    "stylus-loader": "^3.0.2",
    "tiptap": "^1.32.2",
    "tiptap-extensions": "^1.35.2",
    "uid-generator": "^2.0.0",
    "vue": "^2.6.12",
    "vue-apexcharts": "^1.6.2",
    "vue-chartjs": "^3.5.1",
    "vue-clamp": "^0.3.2",
    "vue-i18n": "^8.28.2",
    "vue-router": "^3.6.5",
    "vue-toastification": "^1.7.14",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.7.1",
    "vuex": "^3.6.2",
